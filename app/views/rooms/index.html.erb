<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

    <% @rooms.each do |room| %>
        <%= room.name %></td>
        <%= room.threads %></td>
        <%= link_to 'Show', room %></td>
        <%= link_to 'Edit', edit_room_path(room) %></td>
        <%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>

<br>

<%= link_to 'New Room', new_room_path %>
